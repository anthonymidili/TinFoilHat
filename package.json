{
  "name": "tinfoilhat",
  "version": "2.1.2601",
  "description": "Learn about the Tin Foil Hat Co.",
  "main": "ttfh.co",
  "repository": "ttfh.co",
  "author": "AMidili <tonywinslow@yahoo.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.22",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "esbuild": "^0.27.2",
    "jquery": "^4.0.0",
    "sass": "^1.97.3",
    "what-input": "^5.2.12"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "yarn": ">=4.12.0",
    "node": ">=24.13.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:watch": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets --watch",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css:watch": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules --watch"
  }
}
